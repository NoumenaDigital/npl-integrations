KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=password123
PLATFORM_VERSION=2025.1.1
VITE_NC_APP_NAME=novel
VITE_NC_ORG_NAME=training
NPL_VERSION=1.0
NC_ENGINE_VERSION=2024.2.8
NC_DOMAIN=noumena.cloud

# Configuration
VITE_LOGIN_MODE=KEYCLOAK # DEV_MODE, CUSTOM_OIDC, KEYCLOAK
VITE_DEPLOYMENT_TARGET=LOCAL # LOCAL, NOUMENA_CLOUD

# API configuration
VITE_LOCAL_API_URL=http://localhost:12001 # Use a different port if proxy is required
VITE_CLOUD_API_URL=https://engine-$VITE_NC_ORG_NAME-$VITE_NC_APP_NAME.noumena.cloud

# Auth configuration
# Pick a local deployment login method

# Local deployment settings for DEV MODE (used when DEPLOYMENT_TARGET=LOCAL, LOGIN_MODE=DEV_MODE)
# VITE_LOCAL_AUTH_URL=http://localhost:11000

# Local deployment settings with IAM service (used when DEPLOYMENT_TARGET=LOCAL, LOGIN_MODE=CUSTOM_OIDC; requires reverse proxy)
# VITE_LOCAL_AUTH_URL=http://localhost:11000/realms/$VITE_NC_APP_NAME

# Local deployment settings with keycloak (used when DEPLOYMENT_TARGET=LOCAL, LOGIN_MODE=KEYCLOAK; requires reverse proxy)
VITE_LOCAL_AUTH_URL=http://keycloak:11000

# Pick a noumena cloud deployment login method

# Cloud deployment with local frontend (used when DEPLOYMENT_TARGET=NOUMENA_CLOUD, LOGIN_MODE=CUSTOM_OIDC)
# VITE_CLOUD_AUTH_URL=https://keycloak-$VITE_NC_ORG_NAME-$VITE_NC_APP_NAME.noumena.cloud/realms/$VITE_NC_APP_NAME

# Cloud deployment settings (used when DEPLOYMENT_TARGET=NOUMENA_CLOUD, LOGIN_MODE=KEYCLOAK)
VITE_CLOUD_AUTH_URL=https://keycloak-$VITE_NC_ORG_NAME-$VITE_NC_APP_NAME.noumena.cloud
