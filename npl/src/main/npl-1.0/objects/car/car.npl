package objects.car

enum Color {
    Red, Blue, Yellow, Green, Black
};

@api
protocol[owner] Car(var model: Text, var color: Color) {
    @api
    permission[owner] changeColor(newColor: Color) {
        this.color = newColor;
    };

    permission[owner & *newOwner] transfer() {
        this.owner = newOwner;
    };
};
