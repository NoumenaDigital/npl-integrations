package processes

use objects.car.Car
use objects.car.Color
use objects.iou.Iou

const bob = 'Bob';
const alice = 'Alice';

function demoIou() -> {
    Iou[bob, alice]("demo iou", 100);
};

function demoSettle() -> {
    var car = Car[bob]("VW Beetle", Color.Red);
    var iou = Iou[bob, alice]("settle demo", 5000);

    Settle[alice, bob](iou, car);
};
